  "private": true,
  "dependencies": {
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "is-url-external": "^1.0.3",
    "lodash": "^4.17.5",
    "react": "^16.2.0",
    "react-day-picker": "^7.0.7",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
